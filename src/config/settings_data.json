{
  "current": {
    "button_corners": "square",
    "button_font_size": 16,
    "button_font_weight": "700",
    "button_size": "md",
    "color_schemes": {
      "set-1": {
        "type": "color_scheme",
        "settings": {
          "background": "#FFFFFF",
          "background_accent": "#82B42B",
          "color_accent": "#CBCAA7",
          "color_accent_outline": "#82B42B",
          "color_primary": "#150402",
          "color_secondary": "#000000"
        }
      },
      "set-2": {
        "type": "color_scheme",
        "settings": {
          "background": "#150402",
          "background_accent": "#82B42B",
          "color_accent": "#CBCAA7",
          "color_accent_outline": "#82B42B",
          "color_primary": "#FFFFFF",
          "color_secondary": "#CBCAA7"
        }
      },
      "set-3": {
        "type": "color_scheme",
        "settings": {
          "background": "#F5F5F5",
          "background_accent": "#82B42B",
          "color_accent": "#CBCAA7",
          "color_accent_outline": "#82B42B",
          "color_primary": "#150402",
          "color_secondary": "#000000"
        }
      },
      "set-4": {
        "type": "color_scheme",
        "settings": {
          "background": "#82B42B",
          "background_accent": "#150402",
          "color_accent": "#FFFFFF",
          "color_accent_outline": "#CBCAA7",
          "color_primary": "#150402",
          "color_secondary": "#CBCAA7"
        }
      }
    },
    "corner_radius": "square",
    "font_body": "Roboto",
    "font_heading": "Chakra Petch",
    "font_tagline": "Roboto",
    "font_size_base": 18,
    "font_size_h1": 80,
    "font_size_h2": 56,
    "font_size_h3": 40,
    "font_size_tagline": 16,
    "font_weight_h1": "700",
    "font_weight_h2": "700",
    "font_weight_h3": "700",
    "font_weight_headers": "700",
    "icon_style": "outline",
    "sections": {
      "header": {
        "type": "header",
        "blocks": {
          "logo": {
            "type": "logo",
            "settings": {
              "logo_custom_title": "{{ shop.name }}",
              "logo_image": "booqable://assets/image-store-logo.png",
              "logo_image_floated": "booqable://assets/image-store-logo-white.png"
            },
            "disabled": null
          },
          "menu": {
            "type": "menu",
            "settings": {
              "menu": "main-menu",
              "menu_back_label": "Back",
              "menu_alignment_horizontal": "center",
              "menu_alignment_vertical": "top",
              "menu_opener_position": "left"
            },
            "disabled": null
          },
          "search": {
            "type": "search",
            "settings": {
              "search_closer_label": "Close",
              "search_placeholder": "Search"
            },
            "disabled": null
          },
          "button": {
            "type": "button",
            "settings": {
              "button_label": "Contact us",
              "button_url": "booqable://pages/contact",
              "show_button_desktop": true,
              "show_button_mobile": true
            },
            "disabled": null
          }
        },
        "block_order": [
          "logo",
          "menu",
          "search",
          "button"
        ],
        "settings": {
          "color_scheme": "set-1",
          "header_outline": "none",
          "padding_bottom_desktop": "lg",
          "padding_bottom_mobile": "md",
          "padding_top_desktop": "lg",
          "padding_top_mobile": "md",
          "show_transparency": true,
          "sticky_header": true
        },
        "disabled": null
      }
    },
    "transition_duration": "0.3"
  }
}
